<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.flag.myapplication.hotel">
    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:name="com.example.myweb.util.net.http.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/applogo"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/applogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity android:name="com.flag.myapplication.car.UpdateuserActivity" />
        <activity android:name="com.flag.myapplication.car.UpdateuserinfoActivity" />

        <activity
            android:name="com.flag.myapplication.car.TieziinifActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.flag.myapplication.car.AddjiaotongActivity"
            android:windowSoftInputMode="stateHidden" />


        <activity
            android:name="com.flag.myapplication.car.AddweatherActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.flag.myapplication.car.WeatherActivity"
            android:windowSoftInputMode="stateHidden" />



        <activity
            android:name="com.flag.myapplication.car.RenyuanActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.flag.myapplication.car.AddrenyuanActivity"
            android:windowSoftInputMode="stateHidden" />

        <activity
            android:name="com.flag.myapplication.car.JiaotongActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.flag.myapplication.car.AddimageActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.flag.myapplication.car.LoginActivity"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.flag.myapplication.car.MainActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="com.flag.myapplication.car.RegisterActivity"
            android:windowSoftInputMode="stateHidden" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="guozhaohui.com.picturecropeasy"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>

</manifest>